  <script>
    /* ==========================================================================
       For Tasks 1–10, each task’s algorithm (pseudocode) is shown within its section.
       They use different string manipulation rules to create a password.
    ========================================================================== */
    
    /* ---------------------------
       Task 1 – Use left(x)
       Pseudocode:
         01 subject = input("Enter subject")
         02 password = subject.left(4)
         03 print("Your password is " + password)
       For input: subject = "Computer"
       Expected Trace:
         Row 1: [ "01", "Computer", "", "", "", "" ]
         Row 2: [ "02", "", "left(4)", "Comp", "" ]
         Row 3: [ "03", "", "", "", "", "Your password is Comp" ]
    --------------------------- */
    function checkAnswersTask1() {
      let correctValues = [
        ["01", "Computer", "", "", "", ""],
        ["02", "", "left(4)", "Comp", ""],
        ["03", "", "", "", "", "Your password is Comp"]
      ];
      let table = document.getElementById("traceTableTask1");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
        let inputs = rows[i].getElementsByTagName("input");
        let correct = correctValues[i - 1];
        for (let j = 0; j < inputs.length; j++) {
          inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
        }
      }
    }
    
    /* ---------------------------
       Task 2 – Use right(x)
       Pseudocode:
         01 subject = input("Enter subject")
         02 password = subject.right(3)
         03 print("Your password is " + password)
       For input: subject = "Science"
       Expected Trace:
         Row 1: [ "01", "Science", "", "", "", "" ]
         Row 2: [ "02", "", "right(3)", "nce", "" ]
         Row 3: [ "03", "", "", "", "", "Your password is nce" ]
    --------------------------- */
    function checkAnswersTask2() {
      let correctValues = [
        ["01", "Science", "", "", "", ""],
        ["02", "", "right(3)", "nce", ""],
        ["03", "", "", "", "", "Your password is nce"]
      ];
      let table = document.getElementById("traceTableTask2");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
        let inputs = rows[i].getElementsByTagName("input");
        let correct = correctValues[i - 1];
        for (let j = 0; j < inputs.length; j++) {
          inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
        }
      }
    }
    
    /* ---------------------------
       Task 3 – Use substring(x,y)
       Pseudocode:
         01 subject = input("Enter subject")
         02 password = subject.substring(3,5)
         03 print("Your password is " + password)
       For input: subject = "Computer"
       (Assuming substring(3,5) returns 5 characters starting at index 3: "puter")
       Expected Trace:
         Row 1: [ "01", "Computer", "", "", "", "" ]
         Row 2: [ "02", "", "substring(3,5)", "puter", "" ]
         Row 3: [ "03", "", "", "", "", "Your password is puter" ]
    --------------------------- */
    function checkAnswersTask3() {
      let correctValues = [
        ["01", "Computer", "", "", "", ""],
        ["02", "", "substring(3,5)", "puter", ""],
        ["03", "", "", "", "", "Your password is puter"]
      ];
      let table = document.getElementById("traceTableTask3");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 4 – Combine left(x) and right(x)
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.left(4)
         03 part2 = subject.right(3)
         04 password = part1 + part2
         05 print("Your password is " + password)
       For input: subject = "Computer"
       Expected Trace:
         Row 1: [ "01", "Computer", "", "", "", "" ]
         Row 2: [ "02", "", "left(4)", "Comp", "" ]
         Row 3: [ "03", "", "right(3)", "ter", "" ]
         Row 4: [ "04", "", "Concatenate", "Compter", "" ]
         Row 5: [ "05", "", "", "", "", "Your password is Compter" ]
    --------------------------- */
    function checkAnswersTask4() {
      let correctValues = [
        ["01", "Computer", "", "", "", ""],
        ["02", "", "left(4)", "Comp", ""],
        ["03", "", "right(3)", "ter", ""],
        ["04", "", "Concatenate", "Compter", ""],
        ["05", "", "", "", "", "Your password is Compter"]
      ];
      let table = document.getElementById("traceTableTask4");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 5 – Combine substring(x,y) and left(x)
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.substring(1,3)
         03 part2 = subject.left(3)
         04 password = part1 + part2
         05 print("Your password is " + password)
       For input: subject = "Science"
       For "Science": substring(1,3) → "cie", left(3) → "Sci"
       Expected Trace:
         Row 1: [ "01", "Science", "", "", "", "" ]
         Row 2: [ "02", "", "substring(1,3)", "cie", "" ]
         Row 3: [ "03", "", "left(3)", "Sci", "" ]
         Row 4: [ "04", "", "Concatenate", "cieSci", "" ]
         Row 5: [ "05", "", "", "", "", "Your password is cieSci" ]
    --------------------------- */
    function checkAnswersTask5() {
      let correctValues = [
        ["01", "Science", "", "", "", ""],
        ["02", "", "substring(1,3)", "cie", ""],
        ["03", "", "left(3)", "Sci", ""],
        ["04", "", "Concatenate", "cieSci", ""],
        ["05", "", "", "", "", "Your password is cieSci"]
      ];
      let table = document.getElementById("traceTableTask5");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 6 – Combine substring(x,y) and right(x)
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.substring(4,3)
         03 part2 = subject.right(4)
         04 password = part1 + part2
         05 print("Your password is " + password)
       For input: subject = "Computer Science"
       For "Computer Science": substring(4,3) → "ute", right(4) → "ence"
       Expected Trace:
         Row 1: [ "01", "Computer Science", "", "", "", "" ]
         Row 2: [ "02", "", "substring(4,3)", "ute", "" ]
         Row 3: [ "03", "", "right(4)", "ence", "" ]
         Row 4: [ "04", "", "Concatenate", "uteence", "" ]
         Row 5: [ "05", "", "", "", "", "Your password is uteence" ]
    --------------------------- */
    function checkAnswersTask6() {
      let correctValues = [
        ["01", "Computer Science", "", "", "", ""],
        ["02", "", "substring(4,3)", "ute", ""],
        ["03", "", "right(4)", "ence", ""],
        ["04", "", "Concatenate", "uteence", ""],
        ["05", "", "", "", "", "Your password is uteence"]
      ];
      let table = document.getElementById("traceTableTask6");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 7 – Use left(x), substring(x,y), and right(x) together
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.left(3)
         03 part2 = subject.substring(3,4)
         04 part3 = subject.right(2)
         05 password = part1 + part2 + part3
         06 print("Your password is " + password)
       For input: subject = "Mathematics"
       For "Mathematics": left(3) → "Mat"; substring(3,4) → "hema"; right(2) → "cs"
       Expected Trace:
         Row 1: [ "01", "Mathematics", "", "", "", "" ]
         Row 2: [ "02", "", "left(3)", "Mat", "" ]
         Row 3: [ "03", "", "substring(3,4)", "hema", "" ]
         Row 4: [ "04", "", "right(2)", "cs", "" ]
         Row 5: [ "05", "", "Concatenate", "Mathemacs", "" ]
         Row 6: [ "06", "", "", "", "", "Your password is Mathemacs" ]
    --------------------------- */
    function checkAnswersTask7() {
      let correctValues = [
        ["01", "Mathematics", "", "", "", ""],
        ["02", "", "left(3)", "Mat", ""],
        ["03", "", "substring(3,4)", "hema", ""],
        ["04", "", "right(2)", "cs", ""],
        ["05", "", "Concatenate", "Mathemacs", ""],
        ["06", "", "", "", "", "Your password is Mathemacs"]
      ];
      let table = document.getElementById("traceTableTask7");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 8 – Use substring(x,y) then left(x)
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.substring(2,5)
         03 part2 = subject.left(2)
         04 password = part1 + part2
         05 print("Your password is " + password)
       For input: subject = "Engineering"
       For "Engineering": substring(2,5) → "ginee", left(2) → "En"
       Expected Trace:
         Row 1: [ "01", "Engineering", "", "", "", "" ]
         Row 2: [ "02", "", "substring(2,5)", "ginee", "" ]
         Row 3: [ "03", "", "left(2)", "En", "" ]
         Row 4: [ "04", "", "Concatenate", "gineeEn", "" ]
         Row 5: [ "05", "", "", "", "", "Your password is gineeEn" ]
    --------------------------- */
    function checkAnswersTask8() {
      let correctValues = [
        ["01", "Engineering", "", "", "", ""],
        ["02", "", "substring(2,5)", "ginee", ""],
        ["03", "", "left(2)", "En", ""],
        ["04", "", "Concatenate", "gineeEn", ""],
        ["05", "", "", "", "", "Your password is gineeEn"]
      ];
      let table = document.getElementById("traceTableTask8");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 9 – Use right(x) then substring(x,y)
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.right(4)
         03 part2 = subject.substring(1,3)
         04 password = part1 + part2
         05 print("Your password is " + password)
       For input: subject = "Programming"
       For "Programming": right(4) → "ming", substring(1,3) → "rog"
       Expected Trace:
         Row 1: [ "01", "Programming", "", "", "", "" ]
         Row 2: [ "02", "", "right(4)", "ming", "" ]
         Row 3: [ "03", "", "substring(1,3)", "rog", "" ]
         Row 4: [ "04", "", "Concatenate", "mingrog", "" ]
         Row 5: [ "05", "", "", "", "", "Your password is mingrog" ]
    --------------------------- */
    function checkAnswersTask9() {
      let correctValues = [
        ["01", "Programming", "", "", "", ""],
        ["02", "", "right(4)", "ming", ""],
        ["03", "", "substring(1,3)", "rog", ""],
        ["04", "", "Concatenate", "mingrog", ""],
        ["05", "", "", "", "", "Your password is mingrog"]
      ];
      let table = document.getElementById("traceTableTask9");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
    
    /* ---------------------------
       Task 10 – Use left(x), substring(x,y), and right(x) together
       Pseudocode:
         01 subject = input("Enter subject")
         02 part1 = subject.left(2)
         03 part2 = subject.substring(2,4)
         04 part3 = subject.right(2)
         05 password = part1 + part2 + part3
         06 print("Your password is " + password)
       For input: subject = "Technology"
       For "Technology": left(2) → "Te"; substring(2,4) → "chno"; right(2) → "gy"
       Expected Trace:
         Row 1: [ "01", "Technology", "", "", "", "" ]
         Row 2: [ "02", "", "left(2)", "Te", "" ]
         Row 3: [ "03", "", "substring(2,4)", "chno", "" ]
         Row 4: [ "04", "", "right(2)", "gy", "" ]
         Row 5: [ "05", "", "Concatenate", "Technogy", "" ]
         Row 6: [ "06", "", "", "", "", "Your password is Technogy" ]
    --------------------------- */
    function checkAnswersTask10() {
      let correctValues = [
        ["01", "Technology", "", "", "", ""],
        ["02", "", "left(2)", "Te", ""],
        ["03", "", "substring(2,4)", "chno", ""],
        ["04", "", "right(2)", "gy", ""],
        ["05", "", "Concatenate", "Technogy", ""],
        ["06", "", "", "", "", "Your password is Technogy"]
      ];
      let table = document.getElementById("traceTableTask10");
      let rows = table.rows;
      for (let i = 1; i < rows.length; i++) {
         let inputs = rows[i].getElementsByTagName("input");
         let correct = correctValues[i - 1];
         for (let j = 0; j < inputs.length; j++) {
           inputs[j].style.backgroundColor = (inputs[j].value.trim() === correct[j]) ? "lightgreen" : "lightcoral";
         }
      }
    }
